        <TextBox x:Name="langa" HorizontalAlignment="Left" Height="40" Margin="39,236,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="162" RenderTransformOrigin="0.431,0.465"/>

